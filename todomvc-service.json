{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "todomvc-service"
  },
  "spec": {
    "type": "NodePort",
    "ports": [
      {
        "name": "http",
        "port": 80,
        "protocol": "TCP",
        "targetPort": 80
      }
    ],
    "selector": {
      "app": "todomvc"
    }
  }
}